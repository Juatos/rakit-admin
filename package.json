{
  "name": "@rakit/admin",
  "version": "1.3.13",
  "private": false,
  "type": "module",
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=20",
    "pnpm": ">=10"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "description": "Vue 3 管理后台框架，提供完整的中后台解决方案",
  "keywords": [
    "vue3",
    "admin",
    "typescript",
    "naive-ui",
    "framework",
    "backend-management"
  ],
  "homepage": "https://github.com/Juatos/rakit-admin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Juatos/rakit-admin.git"
  },
  "bugs": {
    "url": "https://github.com/Juatos/rakit-admin/issues"
  },
  "author": "juatos <juatos@proton.me>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./dist/admin.css": "./dist/admin.css"
  },
  "files": [
    "README.md",
    "dist",
    "dist/shims-vue.d.ts"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "vite build",
    "watch": "vite build --watch",
    "lint": "eslint .",
    "type-check": "vue-tsc --noEmit",
    "clean": "rm -rf node_modules dist",
    "preview": "vite preview",
    "prepublishOnly": "pnpm run build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "naive-ui": "^2.42.0"
  },
  "peerDependencies": {
    "@vueuse/core": "^13.0.0",
    "alova": "^3.0.0",
    "dayjs": "^1.11.0",
    "pinia": "^3.0.0",
    "radash": "^12.0.0",
    "vue": "^3.5.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@rakit/conf": "1.2.4",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.32.0",
    "sass-embedded": "^1.89.2",
    "typescript": "^5.8.3",
    "unocss": "^66.3.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^3.0.4"
  }
}
